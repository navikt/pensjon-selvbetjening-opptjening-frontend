  "scripts": {
    "prebuild": "node src/doc/modules/ROOT/doc.js && copyfiles -f src/doc/modules/ROOT/images/*.* public/doc/images",
    "start": "craco start",
    "build": "craco build",
    "build:cypress": "REACT_APP_DECORATOR_URL=https://notinuse.dev.nav.no craco build",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu craco test --env=jsdom",
    "eject": "craco eject",
    "dev": "node run.js",
    "cypress:interactive": "CYPRESS_baseUrl=http://127.0.0.1:3000 cypress open",
    "cy:run:headed": "CYPRESS_baseUrl=http://127.0.0.1:3000 cypress run --headed --no-exit",
    "testcy:headed": "BROWSER=none start-server-and-test dev http://localhost:3000 cy:run:headed",
    "cy:run": "cypress run --browser chrome",
    "testcy": "BROWSER=none start-server-and-test express http-get://127.0.0.1:3000 cy:run",
    "cy:run:update": "cypress run --env updateSnapshots=true",
    "cypress:update": "start-server-and-test dev http://localhost:3000 cy:run:update",
    "express": "node expressServer.js",
    "cypress:docker": "docker run --add-host=host.docker.internal:host-gateway -i -v $PWD:/e2e -w /e2e -e CYPRESS_updateSnapshots=$CYPRESS_updateSnapshots -e CYPRESS_baseUrl=http://host.docker.internal:3000 cypress/included:7.0.0 --browser chrome",
    "test:cycli": "cross-env start-server-and-test express http://$HOST:3000 cypress:docker",
    "cypress:docker:local": "cross-env HOST=127.0.0.1 npm run test:cycli",
    "update:cypress:docker:images": "cross-env HOST=127.0.0.1 CYPRESS_updateSnapshots=true npm run test:cycli"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": "12.18.3",
    "npm": "6.14.6"
